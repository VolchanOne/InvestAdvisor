@model InvestAdvisor.Model.Views.ProjectsViewModel

<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <h3 class="text-themecolor">Проекты</h3>
    </div>
    <div class="col-md-7 align-self-center">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Главная</a></li>
            <li class="breadcrumb-item">Проекты</li>
        </ol>
    </div>
</div>

<div class="container-fluid">
    <div class="row">
        @foreach (var project in Model.Projects)
        {
            <div class="col-lg-3 col-md-4 col-sm-6">
                @Html.Partial("ProjectListItem", project)
            </div>
        }
    </div>
</div>
